st [office, setOffice] = useState(\"\");\n  const [dayOff, setDayOff] = useState(\"\");\n\n  const handleClose1 = () => setShowAdd(false);\n  const handleShow1 = () => setShowAdd(true);\n\n  const handleClose2 = () => setShowDelete(false);\n  const handleShow2 = () => setShowDelete(true);\n\n  const handleClose3 = () => setShowUpdate(false);\n  const handleShow3 = () => setShowUpdate(true);\n\n  const disCourses = (course, position) => {\n    if (props.courses.length === 0) return <span>no courses </span>;\n    else\n      return (\n        <ul class=\"list-group list-group-horizontal\">\n          <li class=\"list-group-item\">{course}</li>\n          <li class=\"list-group-item\">{position}</li>{\" \"}\n        </ul>\n      );\n  };\n\n  useEffect(() => {\n    const courseName = async (arr) => {\n      let coursesNames = [];\n      for (var j = 0; j < props.courses.length; j++) {\n        const c = await getterFetcher.getCourseNameById(\n          props.courses[j].courseId,\n          user.token\n        );\n        coursesNames.push({\n          course: c,\n          position: props.courses[j].position,\n        });\n      }\n      setCourses(coursesNames);\n      return coursesNames;\n    };\n\n    const officeName = async () => {\n      console.log(\"office  \" + props.office);\n      const l = await getterFetcher.getLocationNameById(\n        props.office,\n        user.token\n      );\n      console.log(l);\n      setOffice(l);\n    };\n\n    const dayOffFun = async () => {\n      const d = await props.handleDayOff(props.id);\n      setDayOff(d);\n    };\n    dayOffFun();\n    courseName();\n    officeName();\n  }, [props.courses]);\n\n  const getDay = (num) => {\n    switch (num) {\n      case 0:\n        return \"Sunday\";\n      case 1:\n        return \"Monday\";\n      case 2:\n        return \"Tuesday\";\n      case 3:\n        return \"Wednesday\";\n      case 4:\n        return \"Thursday\";\n      case 5:\n        return \"Friday\";\n      case 6:\n        return \"Saturday\";\n      default:\n    }\n  };\n\n  const popover = (\n    <Popover id=\"popover-basic\">\n      <Popover.Title as=\"h3\">Day Off</Popover.Title>\n      <Popover.Content>\n        this Staff member days off are Friday and {getDay(dayOff)}\n      </Popover.Content>\n    </Popover>\n  );\n\n  return (\n    <div>\n      <div style={{ marginTop: 15 }} className=\"container row\">\n        <div className=\"col-xl-10 offset-3\">\n          <Accordion defaultActiveKey=\"1\">\n            <Card>\n              <Accordion.Toggle as={Card.Header} eventKey=\"0\">\n                <span style={{ fontWeight: \"bold\" }}>\n                  {props.name} ( {props.id} )\n                </span>\n              </Accordion.Toggle>\n              <Accordion.Collapse eventKey=\"0\">\n                <Card.Body>\n                  <dl class=\"row\">\n                    <dt class=\"col-sm-3\">e-mail</dt>\n                    <dd class=\"col-sm-9\">{props.email}</dd>\n\n                    <dt class=\"col-sm-3\">salary</dt>\n                    <dd class=\"col-sm-9\">{props.salary}</dd>\n\n                    <dt class=\"col-sm-3\">Courses</dt>\n                    <dd class=\"col-sm-9\">\n                      {courses && Array.isArray(courses)\n                        ? courses.map((item) => {\n                            return disCourses(item.course, item.position);\n                          })\n                        : []}\n                    </dd>\n\n                    <dt class=\"col-sm-3 text-truncate\">Office location</dt>\n                    <dd class=\"col-sm-9\">{office.name}</dd>\n                  </dl>\n                  <Button\n                    className=\"col col-3\"\n                    variant=\"light\"\n                    onClick={handleShow1}\n                  >\n                    Add course\n                  </Button>\n                  <Button\n                    className=\"col col-3\"\n                    variant=\"light\"\n                    onClick={handleShow2}\n                  >\n                    Delete course\n                  </Button>\n                  <Button\n                    className=\"col col-3\"\n                    variant=\"light\"\n                    onClick={handleShow3}\n                  >\n                    update course\n                  </Button>\n                  <OverlayTrigger\n                    rootClose={true}\n                    trigger=\"click\"\n                    placement=\"top\"\n                    overlay={popover}\n                  >\n                    <Button className=\"col col-3\" variant=\"success\">\n                      Click me to see day off\n                    </Button>\n                  </OverlayTrigger>\n                </Card.Body>\n              </Accordion.Collapse>\n            </Card>\n          </Accordion>\n        </div>\n      </div>\n      <Modal\n        show={showAdd}\n        onHide={handleClose1}\n        backdrop=\"static\"\n        keyboard={false}\n      >\n        <Modal.Header closeButton>\n          <Modal.Title>add Course</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          <Form.Control\n            onChange={(event) => {\n              setCourse(event.target.value);\n            }}\n            type=\"course\"\n            placeholder=\"Enter course\"\n          />\n          <Form.Text className=\"text-muted\">\n            Enter the exact course name.\n          </Form.Text>\n          <Form.Control\n            onChange={(event) => {\n              setType(event.target.value);\n            }}\n            type=\"type\"\n            placeholder=\"Enter type\"\n          />\n          <Form.Text className=\"text-muted\">\n            enter \"instructor\" or \"academic\"\n          </Form.Text>\n          <Alert\n            variant=\"danger\"\n            show={props.showAlert}\n            onClose={() => props.setShowAlert(false)}\n            dismissible\n          >\n            {props.message}\n          </Alert>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button\n            variant=\"secondary\"\n            onClick={() => {\n              handleClose1();\n              props.setShowAlert(false);\n            }}\n          >\n            Close\n          </Button>\n          <Button\n            onClick={() => props.handleAdd(course, props.id, type)}\n            variant=\"success\"\n          >\n            {props.spinner ? (\n              <Spinner\n                as=\"span\"\n                animation=\"border\"\n                size=\"sm\"\n                role=\"status\"\n                aria-hidden=\"true\"\n              />\n            ) : null}\n            Add\n          </Button>\n        </Modal.Footer>\n      </Modal>\n\n      <Modal\n        show={showDelete}\n        onHide={handleClose2}\n        backdrop=\"static\"\n        keyboard={false}\n      >\n        <Modal.Header closeButton>\n          <Modal.Title>Delete Course</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          <Form.Control\n            onChange={(event) => {\n              setCourse(event.target.value);\n            }}\n            type=\"course\"\n            placeholder=\"Enter course\"\n          />\n          <Form.Text className=\"text-muted\">\n            Enter the exact course name.\n          </Form.Text>\n          <Alert\n            variant=\"danger\"\n            show={props.showAlert}\n            onClose={() => props.setShowAlert(false)}\n            dismissible\n          >\n            {props.message}\n          </Alert>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button\n            variant=\"secondary\"\n            onClick={() => {\n              handleClose2();\n              props.setShowAlert(false);\n            }}\n          >\n            Close\n          </Button>\n          <Button\n            onClick={() => {\n              props.handleDelete(course, props.id);\n              setCourse(\"\");\n            }}\n            variant=\"danger\"\n          >\n            {props.spinner ? (\n              <Spinner\n                as=\"span\"\n                animation=\"border\"\n                size=\"sm\"\n                role=\"status\"\n                aria-hidden=\"true\"\n              />\n            ) : null}\n            Delete\n          </Button>\n        </Modal.Footer>\n      </Modal>\n      <Modal\n        show={showUpdate}\n        onHide={handleClose3}\n        backdrop=\"static\"\n        keyboard={false}\n      >\n        <Modal.Header closeButton>\n          <Modal.Title>update Course</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          <Form.Control\n            onChange={(event) => {\n              setCourse(event.target.value);\n            }}\n            type=\"course\"\n            placeholder=\"Enter course\"\n          />\n          <Form.Text className=\"text-muted\">\n            Enter the exact course name.\n          </Form.Text>\n          <Form.Control\n            onChange={(event) => {\n              setNewStaff(event.target.value);\n            }}\n            type=\"course\"\n            placeholder=\"Enter new staff member\"\n          />\n          <Form.Text className=\"text-muted\">\n            Enter the new staff member to swap courses.\n          </Form.Text>\n          <Alert\n            variant=\"danger\"\n            show={props.showAlert}\n            onClose={() => props.setShowAlert(false)}\n            dismissible\n          >\n            {props.message}\n          </Alert>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button\n            variant=\"secondary\"\n            onClick={() => {\n              handleClose3();\n              props.setShowAlert(false);\n            }}\n          >\n            Close\n          </Button>\n          <Button\n            onClick={() => {\n              props.handleUpdate(course, props.id, newStaff);\n              setCourse(\"\");\n            }}\n            variant=\"primary\"\n          >\n            {props.spinner ? (\n              <Spinner\n                as=\"span\"\n                animation=\"border\"\n                size=\"sm\"\n                role=\"status\"\n                aria-hidden=\"true\"\n              />\n            ) : null}\n            update\n          </Button>\n        </Modal.Footer>\n      </Modal>\n    </div>\n  );\n};\n\nexport default ViewStaff;\n","/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/HrStaffMember.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/home/CoordinatorHome.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/common/Schedule.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/misc/NavBar.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/instructor/StaffTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/academic/AcademicRequest.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/HrCourseTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/HrFacultyTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/HrLocationTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/common/Cell.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/staff/UpdateProfile.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/HrDepartmentTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/HrStaffMemberTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/common/Profile.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/common/Request.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/academic/AttendanceRecord.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/common/Record.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/academic/RequestForm.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hod/RequestsHomePage.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/SignInOut.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hr/ViewAttendanceRecords.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/coordinator/CoordinatorReqTemp.jsx",[],"/home/baroudy/Projects/advancedLab/guc-portal/client/src/components/hod/ViewStaff.jsx",[],{"ruleId":"239","replacedBy":"240"},{"ruleId":"241","replacedBy":"242"},{"ruleId":"243","severity":1,"message":"244","line":81,"column":6,"nodeType":"245","endLine":81,"endColumn":21,"suggestions":"246"},"no-native-reassign",["247"],"no-negated-in-lhs",["248"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'props' and 'user.token'. Either include them or remove the dependency array. However, 'props' will change when *any* prop changes, so the preferred fix is to destructure the 'props' object outside of the useEffect call and refer to those specific props inside useEffect.","ArrayExpression",["249"],"no-global-assign","no-unsafe-negation",{"desc":"250","fix":"251"},"Update the dependencies array to be: [props, props.courses, user.token]",{"range":"252","text":"253"},[2607,2622],"[props, props.courses, user.token]"]